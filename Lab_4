//Task 5

let season = 3

switch season {
case 1:
    print("–∑–∏–º–∞")
case 2:
    print("–≤–µ—Å–Ω–∞")
case 3:
    print("–ª–µ—Ç–æ")
case 4:
    print("–æ—Å–µ–Ω—å")
default:
    print("–Ω–∏—á–µ–≥–æ")
}

//Task 6

let friend = "Aida"

switch friend {
case "Arman":
    print("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!")
case "Aida":
    print("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!")
default:
    print("–í–∞—Å –Ω–µ—Ç –≤ —Å–ø–∏—Å–∫–µ")
}

//–ø—Ä–æ–≤–µ—Ä–∏–º —Ç–µ–ø–µ—Ä—å –Ω–∞–æ–±–æ—Ä–æ—Ç:

let friend1 = "Maksat"

switch friend1 {
case "Arman":
    print("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!")
case "Aida":
    print("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!")
default:
    print("–í–∞—Å –Ω–µ—Ç –≤ —Å–ø–∏—Å–∫–µ")
}
//—Ä–∞–±–æ—Ç–∞–µ—Ç, –ø–æ–º–µ–Ω—è–ª–∞ –Ω–∞ friend1, —Ç–∞–∫ –∫–∞–∫ –≤—ã–¥–∞–≤–∞–ª –æ—à–∏–±–∫—É, –≤–µ–¥—å friend —É–∂–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç


//Task 7

let ticketAge = 20

switch ticketAge {
case 0...2:
    print("–ú–ª–∞–¥–µ–Ω–µ—Ü")
case 2...14:
    print("–î–µ—Ç—Å–∫–∏–π –±–∏–ª–µ—Ç")
case 14...:
    print("–í–∑—Ä–æ—Å–ª—ã–π –±–∏–ª–µ—Ç")
default:
    print("–î—Ä—É–≥–æ–≥–æ –±–∏–ª–µ—Ç–∞ –Ω–µ—Ç")
}

//Task 8

var expensiveFood: String

let beshbarmak = 7000
let sushi = 5000

if beshbarmak > sushi {
    expensiveFood = "Beshbarmak"
} else {
    expensiveFood = "Sushi"
}

print(expensiveFood)

//Task 9

let dayOfTheWeek = 2

switch dayOfTheWeek {
case 1:
    print("–°–µ–≥–æ–¥–Ω—è –¥–µ–Ω—å –≥—Ä—É–¥–∏, –ø–ª–µ—á –∏ —Ç—Ä–∏—Ü–µ–ø—Å–æ–≤, —Å—Ö–æ–¥–∏ –≤ –∑–∞–ª –∏ –Ω–µ –∑–∞–±—É–¥—å –≤—ã–ø–∏—Ç—å –∫—Ä–µ–∞—Ç–∏–Ω")
case 2:
    print("–°–µ–≥–æ–¥–Ω—è –¥–µ–Ω—å —Å–ø–∏–Ω—ã –∏ –±–∏—Ü–µ–ø—Å–æ–≤, —Å–¥–µ–ª–∞–π –∑–∞—Ä–∞–Ω–µ–µ –æ–±–µ–¥ –∏ —Å—Ö–æ–¥–∏ –≤ –∑–∞–ª")
case 3:
    print("–°–µ–≥–æ–¥–Ω—è —Å—Ä–µ–¥–∞, –∑–Ω–∞—á–∏—Ç –¥–µ–Ω—å –Ω–æ–≥, –Ω–µ –∑–∞–±—É–¥—å –≤–∑—è—Ç—å –ª—è–º–∫–∏ –¥–ª—è —Ç—è–≥–∏ –∏ 2 –ª–∏—Ç—Ä–∞ –≤–æ–¥—ã")
case 4:
    print("–°–µ–≥–æ–¥–Ω—è rest day, –æ—Ç–¥—ã—Ö–∞–π, –Ω–æ –Ω–µ –ø–µ—Ä–µ–µ–¥–∞–π, —Å–æ–±–ª—é–¥–∞–π –¥–µ—Ñ–∏—Ü–∏—Ç –∫–∞–ª–æ—Ä–∏–π")
case 5:
    print("–°–µ–≥–æ–¥–Ω—è –ø—è—Ç–Ω–∏—Ü–∞, —Å–Ω–æ–≤–∞ —É –Ω–∞—Å push day, –≤—ã–ø–µ–π –ø—Ä–æ—Ç–µ–∏–Ω–æ–≤—ã–π –∫–æ–∫—Ç–µ–π–ª—å –ø–æ—Å–ª–µ —Ç—Ä–µ–Ω–∏—Ä–æ–≤–æ–∫")
case 6:
    print("–°–µ–≥–æ–¥–Ω—è —Å—É–±–±–æ—Ç–∞, pull day, –ø–æ—Å–ª–µ –∑–∞–ª–∞ —Å—Ö–æ–¥–∏ –Ω–∞ –ø–ª–∞–≤–∞–Ω–∏–µ –∏ –ø–æ–ø–ª—ã–≤–∏ –≤ —Å—Ç–∏–ª–µ butterfly –±–æ–ª–µ–µ 5 –∫—Ä—É–≥–æ–≤")
case 7:
    print("–°–µ–≥–æ–¥–Ω—è –¥–µ–Ω—å –Ω–æ–≥, —Å—Ö–æ–¥–∏ –≤ –∑–∞–ª, –ø–æ—Ç–æ–º –∫–∞—Ä–¥–∏–æ –Ω–∞ –≤–µ–ª–æ—Å–∏–ø–µ–¥–µ –≤–µ—á–µ—Ä–æ–º –≤ –ø–∞—Ä–∫–µ –æ–∫–æ–ª–æ 15 –∫–º")
default:
    print("–û—Ç–¥—ã—Ö–∞–π")
}

//Task 10

let metroStation = "a"

let exactMetroStation = metroStation.uppercased() //–≤ –ª—é–±–æ–º —Å–ª—É—á–∞–µ –æ–Ω –±—É–¥–µ—Ç –ø—Ä–µ–æ–±—Ä–∞–∑–æ–≤—ã–≤–∞—Ç—å –±—É–∫–≤—ã –≤ –∑–∞–≥–ª–∞–≤–Ω—ã–µ

switch exactMetroStation {
case "A":
    print("–°—Ç–∞–Ω—Ü–∏—è –ê. –í—Ä–µ–º—è 12:00")
case "B":
    print("–°—Ç–∞–Ω—Ü–∏—è B. –í—Ä–µ–º—è 13:00")
case "C":
    print("–°—Ç–∞–Ω—Ü–∏—è C. –í—Ä–µ–º—è 14:00")
case "D":
    print("–°—Ç–∞–Ω—Ü–∏—è D. –í—Ä–µ–º—è 15:00")
case "E":
    print("–°—Ç–∞–Ω—Ü–∏—è E. –í—Ä–µ–º—è 16:00")
default:
    print("–¢–∞–∫–æ–π —Å—Ç–∞–Ω—Ü–∏–∏ –Ω–µ—Ç, –Ω–∞–ø–∏—à–∏—Ç–µ –¥—Ä—É–≥—É—é –±—É–∫–≤—É")
}

//Task 11

let fingerNum = 1

switch fingerNum {
case 1:
    print("üëçüèª")
case 2:
    print("‚òùüèª")
case 3:
    print("–°—Ä–µ–¥–Ω–∏–π –ø–∞–ª–µ—Ü")
case 4:
    print("–ë–µ–∑—ã–º—è–Ω–Ω—ã–π –ø–∞–ª–µ—Ü")
case 5:
    print("–ú–∏–∑–∏–Ω–µ—Ü")
default:
    print("‚úãüèª")
}
