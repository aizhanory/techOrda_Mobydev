//Task 1

//1.
    print(9 == 9)
    print(true)
//2.
    print(9 != 9)
    print(false)

//3.
    print(47 > 90)
    print(false)

//4.
    print(47 < 90)
    print(true)

//5.
    print(4 <= 4)
    print(true)

//6.
    print(4 >= 5)
    print(false)

//7.
    print((47 > 90) && (47 < 90))
    print(false)

//8.
    print((47 > 90) || (47 < 90))
    print(true)

//9.
    print(!true)
    print(false)


//Task 2

var tenge = 0

if (tenge == 0){
    print("–ò–∑–≤–∏–Ω–∏, –Ω–æ —Ç—ã –Ω–∞ –º–µ–ª–∏!")
}

tenge = 300

if (tenge == 0){
    print("–ò–∑–≤–∏–Ω–∏, –Ω–æ —Ç—ã –Ω–∞ –º–µ–ª–∏!")
} else {
    print("–í–∞—É, —É —Ç–µ–±—è –µ—Å—Ç—å –¥–µ–Ω—å–≥–∏ –Ω–∞ –ø–∏—Ä–æ–∂–∫–∏!")
}

tenge = 2000

if (tenge == 0){
    print("–ò–∑–≤–∏–Ω–∏, –Ω–æ —Ç—ã –Ω–∞ –º–µ–ª–∏!")
} else if (tenge < 400) {
    print("–í–∞—É, —É —Ç–µ–±—è –µ—Å—Ç—å –¥–µ–Ω—å–≥–∏ –Ω–∞ –ø–∏—Ä–æ–∂–∫–∏!")
} else {
    print("–ï–¥–µ–º –Ω–∞ –¢–∞–∫—Å–∏!")
}

//Task 3

